#!/usr/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'lextend==0.1','console_scripts','lextend_webfrontend'
__requires__ = 'lextend==0.1'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('lextend==0.1', 'console_scripts', 'lextend_webfrontend')()
    )
